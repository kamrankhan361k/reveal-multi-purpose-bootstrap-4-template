{{> comment title="Header"}}

<section class="position-relative bg-dark text-white overflow-hidden">
    <div class="cover cover-overlay cover-gradient-vertical-bottom rellax rellax-move" data-rellax-speed="-1" style="background-image: url({{data.img}}general/4.jpg)"></div>
